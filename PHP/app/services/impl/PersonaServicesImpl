<?php

namespace Services;

use Services\PersonaServices;
use Repositories\PersonaRepositories;

class PersonaServicesImpl implements PersonaServices
{

    private $personaRepository;

    public function __construct(PersonaRepositories $personaRepository)
    {
        $this->personaRepository = $personaRepository;
    }

    public function createPersona($data)
    {
        return $this->personaRepository->createPersona($data);
    }

    public function updatePersona($id, $data)
    {
        return $this->personaRepository->updatePersona($id, $data);
    }

    public function deletePersona($data)
    {

        return $this->personaRepository->deletePersona($data);
    }

    public function getPersona($data)
    {
        return $this->personaRepository->getPersona($data);
    }

    public function getAllPersonas()
    {
        return $this->personaRepository->getAllPersonas();
    }
} {
}
